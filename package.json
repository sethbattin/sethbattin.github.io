{
  "name": "sethbattin.github.io",
  "version": "1.0.0-wip.0",
  "description": "Personal blog",
  "main": " ",
  "directories": {
    "doc": "docs"
  },
  "type": "module",
  "scripts": {
    "mark": "node $npm_config_xmod node_modules/.bin/marked --",
    "vizreg": "node $npm_config_xmod scripts/vizreg.mjs",
    "publish": "node $npm_config_xmod scripts/publish.mjs",
    "start": "npx serve docs",
    "test": "node $npm_config_xvmmod node_modules/.bin/jest "
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sethbattin/sethbattin.github.io.git"
  },
  "author": "Seth Battin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sethbattin/sethbattin.github.io/issues"
  },
  "homepage": "https://github.com/sethbattin/sethbattin.github.io#readme",
  "dependencies": {
    "marked": "^4.0.10"
  },
  "devDependencies": {
    "jest": "^27.4.7",
    "puppeteer": "^13.0.1",
    "serve": "^13.0.2",
    "spellchecker": "^3.7.1"
  }
}
